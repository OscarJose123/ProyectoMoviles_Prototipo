<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_page11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.oscarjosmoreira.prototipoaplicacion.Page11"
    android:background="@android:color/holo_green_light">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="34dp"
        android:id="@+id/textView49"
        android:text="¿Cual de las siguientes imagenes es una señal preventiva?"
        android:textColor="@android:color/background_dark" />

    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkBox30"
        android:layout_alignParentStart="true"
        android:layout_marginTop="75dp"
        android:id="@+id/checkBox31" />

    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkBox31"
        android:layout_alignParentStart="true"
        android:layout_marginTop="46dp"
        android:id="@+id/checkBox32" />

    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="59dp"
        android:id="@+id/checkBox30"
        android:layout_below="@+id/textView49"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/contramano"
        android:layout_above="@+id/checkBox31"
        android:layout_toEndOf="@+id/checkBox30"
        android:layout_marginStart="85dp"
        android:id="@+id/imageView40" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/derecha"
        android:layout_above="@+id/checkBox32"
        android:layout_alignStart="@+id/imageView40"
        android:id="@+id/imageView41" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="100dp"
        app:srcCompat="@drawable/alto"
        android:id="@+id/imageView42"
        android:layout_below="@+id/imageView41"
        android:layout_alignStart="@+id/imageView41" />

    <Button
        android:text="Menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="42dp"
        android:layout_marginBottom="25dp"
        android:id="@+id/button37"
        android:background="@android:color/holo_red_dark"
        android:textColor="@android:color/white" />

    <Button
        android:text="Siguiente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button37"
        android:layout_alignStart="@+id/imageView42"
        android:id="@+id/button38"
        android:background="@android:color/holo_red_dark"
        android:textColor="@android:color/white" />
</RelativeLayout>
