<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_page5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.oscarjosmoreira.prototipoaplicacion.Page5"
    android:background="@android:color/holo_green_light">

    <TextView
        android:text="¿Qué significa el término  “culposo” en materia de accidentes de tránsito? "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="63dp"
        android:id="@+id/textView26"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textColor="@android:color/black" />

    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkBox13"
        android:layout_alignParentStart="true"
        android:layout_marginTop="45dp"
        android:id="@+id/checkBox14" />

    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/checkBox14"
        android:layout_alignParentStart="true"
        android:layout_marginTop="40dp"
        android:id="@+id/checkBox15" />

    <TextView
        android:text=" Que es responsable por actuar con intención de provocar un daño. "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView33"
        android:layout_below="@+id/checkBox13"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/background_dark" />

    <CheckBox
        android:text="CheckBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="57dp"
        android:id="@+id/checkBox13"
        android:layout_below="@+id/textView26"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Que no estaba habilitado para conducir."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView37"
        android:layout_marginStart="11dp"
        android:layout_below="@+id/checkBox15"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/background_dark" />

    <TextView
        android:text=" Que no obró con intención de provocar un daño.  "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView35"
        android:layout_below="@+id/checkBox14"
        android:layout_alignParentStart="true"
        android:textColor="@android:color/background_dark" />

    <Button
        android:text="Menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView37"
        android:layout_marginStart="46dp"
        android:layout_marginBottom="34dp"
        android:id="@+id/button25"
        android:background="@android:color/holo_red_dark"
        android:textColor="@android:color/white" />

    <Button
        android:text="Siguiente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button25"
        android:layout_alignEnd="@+id/textView35"
        android:layout_marginEnd="28dp"
        android:id="@+id/button26"
        android:background="@android:color/holo_red_dark"
        android:textColor="@android:color/white" />
</RelativeLayout>
